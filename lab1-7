#1
n=int(input("n="))
i=0
od=0
mas=[]
while i<n:
   
    
    i+=1
    mas.append(i)
print(mas) 

aa=str(mas)

import re
mat=re.findall(r"9",aa)
print(len(mat))

#2
a = int(input("Ile chczesz liczb: "))
list = []
i=0
d=0
while i < a:
    m = int(input("Wprowadz liczby: "))
    list.append(m)
    i+=1
print(list)

for i in list:
    if i == 0:
        list.sort(key=lambda i:i==0)
print(list)


