lst=[]
while True:  
    
    b=int(input("b="))

    if b<0:
        break
    else:
        lst.append(b)    
            

print(lst)

i = lst.index(0)


i2 = lst.index(0, i + 1)

lst[i:i2]

a=sum(lst[i:i2])
print(a)

    






